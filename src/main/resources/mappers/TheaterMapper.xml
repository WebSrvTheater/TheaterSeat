<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.websrv.theaterseat.dao.TheaterMapper">

    <select id="selectTheaterAndRoom" resultType="map">
        select theaterName, roomName from theater,room where theater.t_idx = room.t_idx
    </select>

</mapper>
